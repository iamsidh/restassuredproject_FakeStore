<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Fake Store API Test Suite">
    <listeners>
        <listener class-name="utilities.ExtentReporter"></listener>
    </listeners>
    <test thread-count="5" name="Product Tests">
        <classes>
            <class name="testcases.ProductTests"/>
        </classes>
    </test>

    <test thread-count="5" name="Users Test">
        <classes>
            <class name="testcases.UsersTest"/>
        </classes>
    </test>

    <test thread-count="5" name="Cart Test">
        <classes>
            <class name="testcases.CartTests"/>
        </classes>
    </test>

    <test thread-count="5" name="Login Test">
        <classes>
            <class name="testcases.LoginTestcases"/>
        </classes>
    </test>
</suite>
